func _process(delta):
	var velocity = Vector2.ZERO;
	
	match(Input.is_action_pressed):
		"move_up":
			velocity.y += 1
		"move_down":
			velocity.y -= 1
		"move_left":
			velocity.x -= 1
		"move_right":
			velocity.x += 1 	
			
			
	if velocity.length() > 0:
		velocity = velocity.normalized() * speed
		$AnimatedSprite2D.play()
	else:
		$AnimatedSprite2D.stop()		
